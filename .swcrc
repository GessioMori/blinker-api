{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
      "@user/*": ["modules/user/*"],
      "@auth/*": ["modules/auth/*"],
      "@/*": ["*"]
    },
    "baseUrl": "./src"
  },
  "module": {
    "type": "commonjs"
  }
}
