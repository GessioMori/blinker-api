    "nodemon": "^2.0.20",
    "prisma": "^4.10.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.10.0",
    "argon2": "^0.30.3",
    "connect-redis": "^6.1.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "redis": "^4.6.4",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "zod": "^3.20.6"
  }
